<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="@color/primary_500"
                android:dashWidth="8dp"
                android:dashGap="2dp"/>
            <corners android:radius="20dp"/>
            <padding
                android:left="4dp"
                android:top="4dp"
                android:right="4dp"
                android:bottom="4dp"/>
        </shape>
    </item>
</layer-list>
